import java.util.* ;
public class ArraylistExample {
	public static void main(String[] args)
	       {
	        ArrayList <Integer> alist = new ArrayList <Integer>();
	        alist.add(3);
	        alist.add(1);
	        alist.add(7);
	        alist.add(20);
	        alist.add(5);
	        int val = 0;
	      while (alist.size() > val) 
	      {    
	    	  System.out.println(alist.get(val));
	             val++ ;
	        }
	    }
	}
